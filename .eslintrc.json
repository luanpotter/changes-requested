
{
    "plugins": ["@typescript-eslint"],
    "extends": ["plugin:github/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "rules": {
      "import/no-namespace": "off"
    },
    "env": {
      "node": true,
      "es6": true
    }
  }